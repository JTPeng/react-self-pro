.gif
	display: inline-block
	width 60px
	border-radius: 45px 0 0 45px
	height 45px
	text-align: center
	line-height: 45px
	position fixed
	right 0
	bottom 100px
	z-index: 1000
	background-color #ffffff
	.icon-gift
		font-size: 40px
		color #f00
.up
	display: inline-block
	width 40px
	border-radius: 50%
	height 40px
	text-align: center
	line-height: 40px
	position fixed
	right 20px
	bottom 40px
	z-index: 1000
	background-color #ebebeb
	.icon-up
		font-size: 40px
		color #f00
.mainHeader
	position fixed
	top 0
	z-index 1000
	background-color #ffffff
	border-bottom 1px solid #d9d9d9
	.headerTop
		position relative
		z-index 2
		height 50px
		padding 5px 10px
		display flex
		flex-flow row nowrap
		align-items center
		background-color #ffffff
		.logo
			width 80px
			height 20px
			font-size 20px
			margin-right 6px
		input
			height 30px
			background-color #ededed
			border-radius 5px
			display flex
			flex-flow row nowrap
			align-items center
			justify-content center
			font-size 16px
			margin-right 10px
			padding 2px 2px 0px 25px
			background-image url('./images/search.png')
			background-size 18px
			background-position-y center
			background-position-x 5px
			background-repeat no-repeat
		.login
			width 50px
			height 25px
			text-align center
			line-height 25px
			border 1px solid red
			border-radius 5px
	.nav
		position relative
		.tabs
			width 80%
		.tabsListContainer
			position absolute
			left 0
			top 0
			.tabsListHeader
				padding 0 5px 0 20px
				height 43.5px
				line-height 43px
				display flex
				align-items center
				justify-content space-between
				.all
					font-size 12px
				.arrows
					width 20px
					padding 0 10px 0 25px
					transition all 1s
					background-color #fff
					z-index 99
			.tabsList
				display flex
				flex-wrap wrap
				padding-left 20px
				background-color #fff
				.tabsItem
					margin 10px 10px 10px 0
					width 20%
					height 25px
					line-height 25px
					text-align center
					border 1px solid #eee
					border-radius 4px
					font-size 12px
					background-color rgba(238, 238, 238, 0.3)
			.mask
				width 100%
				height 200%
				position absolute
				z-index: 2000
				left 0
				top 185px
				background-color: rgba(127, 127, 127,.5)
